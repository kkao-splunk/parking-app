<form>
  <label>Maps</label>
  <fieldset submitButton="false"></fieldset>
  <row>
    <panel>
      <input type="text" token="field2">
        <label>Time</label>
      </input>
      <input type="text" token="field3">
        <label>Address</label>
      </input>
      <input type="multiselect" token="field4">
        <label>Parking</label>
        <choice value="meter">Meter</choice>
        <choice value="garage">Garage</choice>
      </input>
      <map>
        <search>
          <query>| inputlookup meters.csv | rex field=LOCATION "\((?&lt;lat&gt;[^,]+), (?&lt;lon&gt;[^\)]+)\)"</query>
          <earliest>-24h@h</earliest>
          <latest>now</latest>
        </search>
        <option name="drilldown">none</option>
        <option name="mapping.map.center">(37.7749,-122.4194)</option>
        <option name="mapping.map.scrollZoom">1</option>
        <option name="mapping.map.zoom">14</option>
        <option name="mapping.tileLayer.maxZoom">100</option>
        <option name="mapping.tileLayer.url">http://mt1.google.com/vt/lyrs=m&amp;x={x}&amp;y={y}&amp;z={z}</option>
        <option name="mapping.type">marker</option>
      </map>
    </panel>
  </row>
</form>